{
    "name": "genealabs/laravel-impersonator",
    "description": "Impersonate users in your Laravel and Nova apps.",
    "license": "MIT",
    "authors": [
        {
            "name": "GeneaLabs, LLC",
            "email": "hello@genealabs.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "illuminate/config": "^10.0",
        "illuminate/console": "^10.0",
        "illuminate/contracts": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/session": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/view": "^10.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",       
        "laravel/browser-kit-testing": "^7.0",
        "php-coveralls/php-coveralls" : "^2.2",
        "phpmd/phpmd": "^2.8",
        "laravel/nova": "^3.12"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "GeneaLabs\\LaravelImpersonator\\": "src/",
            "GeneaLabs\\LaravelImpersonator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelImpersonator\\Providers\\Service",
                "GeneaLabs\\LaravelImpersonator\\Providers\\Tool"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
